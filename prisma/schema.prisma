generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Placeholder {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  others Other[]
}

model Other {
  placeholder Placeholder @relation(fields: [placeholderId], references: [id])
  placeholderId Int @id
  @@index([placeholderId])
}