generator client {
  provider = "prisma-client-js"
}

// generator prismary {
//   provider = "prismary"
// }

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  bases Baseiooo[]
}

model Baseiooo {
  user User @relation(fields: [userId], references: [id])
  userId Int @id
}