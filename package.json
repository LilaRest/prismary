{
  "name": "prismary",
  "version": "1.6.1",
  "license": "MIT",
  "author": "Lila Rest",
  "homepage": "https://github.com/LilaRest/prismary",
  "keywords": [
    "prisma",
    "generator",
    "trpc",
    "zod",
    "casl"
  ],
  "main": "dist/index.js",
  "bin": {
    "prismary": "dist/generator.js"
  },
  "scripts": {
